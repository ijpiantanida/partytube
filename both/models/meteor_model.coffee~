MeteorModel =
 fromModel: (data) ->
  alert("Test");
  instance = new this()
  instance._meteor_model = data
  instance[name] = data[name] for name in data
  instance
 extended: ->
  @include
   update: -> 
    @model.update(@_id,@mapToData)
   mapToData: ->
    @_meteor_model[name] = @[name] for name in @_meteor_model
    @_meteor_model

