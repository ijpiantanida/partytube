MeteorModel =
	fromModel: (data) ->
		@_meteor_model = data;
		for (var name in data)
			@[name] = data[name];
		
	extended: ->
		@include
			update: ->
				@model.update(@_id,@mapToData);
			mapToData: ->
				for (var name in @_meteor_model)
					@_meteor_model[name] = @[name];
				@_meteor_model;

